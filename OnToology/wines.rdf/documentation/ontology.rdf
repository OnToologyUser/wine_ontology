                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/atorres/ontologies/2023/3/spanish_wine_ontology#Variety"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Wine class to instance.</rdfs:comment>
        <rdfs:sameAs>https://dbpedia.org/page/Wine</rdfs:sameAs>
